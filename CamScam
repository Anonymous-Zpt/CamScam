Adios() {
		sleep 0.2
		echo -e "\n${v1}\n[$b1+$v1]$b Finished\n$v1[$b1+$v1]$b Created by Anonymous Zpt\n$v1[$b1+$v1]$b Gracias por usar mi herramienta\n$v1[$b1+$v1]$b servicio de ayuda https://t.me/Anonymous_Zpt$b\n"
