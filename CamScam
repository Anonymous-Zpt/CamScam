titulo=$(cat .VideoYT |grep -i '<meta property="og:title"')
descripcion=$(cat .VideoYT |grep -i '<meta property="og:description"')
url=$(cat .VideoYT |grep -i '<meta property="og:url"')
nombre=$(cat .VideoYT |grep -i '<meta itemprop="name"')
